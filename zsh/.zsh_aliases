# Ls aliases
alias ls='ls --color=auto'
alias lv='ls -1 --color=auto'
alias la='ls -la --color=auto'
alias lsl='ls -l --color=auto'

# Navigation aliases
alias hd='cd ~'
alias win='cd /mnt/c/Users/BTUKUS/Desktop'

# Dotfiles
alias dot='cd ~/dotfiles'
alias bw='cd ~/dotfiles/brew'
alias tm='cd ~/dotfiles/tmux'

alias su='source ~/.zshrc'
alias shell='cd ~/dotfiles/zsh'

alias f='nvim'
alias fs='nvim --startuptime /tmp/nvim-startuptime'
alias nc='cd ~/dotfiles/nvim/.config/nvim/'
alias ncc='cd ~/dotfiles/nvim/.config/nvim/lua/core'
alias ncp='cd ~/dotfiles/nvim/.config/nvim/lua/plugins'

alias RM='sudo rm -r'

alias cdb='cd -'
alias GD='temp=$(pwd)'
